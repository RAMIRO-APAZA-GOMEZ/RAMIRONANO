function imprimir(recolocar) {  

for (x=0;x<document.images.length;x++){
    if (recolocar)
      document.images[x].className = "imagen";
    else
      document.images[x].style.visibility = "visible";
  }
  
if (window.print)
window.print()
else
alert("Para imprimir presione Crtl+P.");
}

